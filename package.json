  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npx tsc -p .",
    "watch": "npx tsc -w -p .",
    "clean": "rm -rf dist"
  },
  "bugs": {
    "url": "http://github.com/inkytonik/monto-vscode/issues"
  },
  "repository": {
    "type": "git",
