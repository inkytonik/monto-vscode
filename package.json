  },
  "keywords": [
    "Monto",
    "text editing",
    "structured editor views"
  ],
  "author": "Anthony Sloane <inkytonik@gmail.com>",
  "license": "MPL-2.0",
  "dependencies": {
    "vscode-languageclient": "^5.2.1"
  },
  "devDependencies": {
    "typescript": "^4.1.0",
    "@types/node": "^8.0.0",
    "@types/vscode": "^1.46.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/lib"
  ]
}
